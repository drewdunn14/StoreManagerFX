<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="300.0" style="-fx-background-color: #0C2340;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SellerController">
   <children>
      <Pane prefHeight="50.0" prefWidth="300.0" style="-fx-background-color: #808080;">
         <children>
            <Label fx:id="viewText" alignment="CENTER" layoutX="97.0" layoutY="15.0" text="Seller View">
               <font>
                  <Font size="19.0" />
               </font>
            </Label>
            <Button fx:id="backButton" layoutX="14.0" layoutY="12.0" mnemonicParsing="false" text="Back" />
         </children>
      </Pane>
      <Button fx:id="loadButton" layoutX="32.0" layoutY="344.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="108.0" text="Load Product" />
      <Button fx:id="saveButton" layoutX="160.0" layoutY="344.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="108.0" text="Save Product" />
      <Label layoutX="35.0" layoutY="65.0" text="Enter Product ID to Load/Edit Items" textFill="WHITE" />
      <TextField fx:id="idText" alignment="CENTER" layoutX="65.0" layoutY="108.0" promptText="Enter Product ID" />
      <TextField fx:id="nameText" alignment="CENTER" layoutX="65.0" layoutY="168.0" promptText="Product Name" />
      <TextField fx:id="priceText" alignment="CENTER" layoutX="65.0" layoutY="223.0" promptText="Product Price" />
      <TextField fx:id="quantityText" alignment="CENTER" layoutX="65.0" layoutY="283.0" promptText="Quantity Available" />
      <Label fx:id="errorText" layoutX="70.0" layoutY="86.0" textFill="RED" />
      <Label fx:id="statusText" layoutX="68.0" layoutY="316.0" textFill="WHITE" />
   </children>
</AnchorPane>
